{
  "name": "rdfs",
  "displayName": "React Dev Folders",
  "publisher": "ThamilThennarasu",
  "version": "1.0.0",
  "description": "React folder management for developers",
  "main": "./out/extension.js",
  "icon": "./extension/icon.png",
  "keywords": [
    "react",
    "rdfs",
    "devfolder",
    "folder",
    "dev",
    "folders",
    "workspace",
    "toggle"
  ],
  "engines": {
    "vscode": "^1.108.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Thamil001/rdfs.git"
  },
  "bugs": {
    "url": "https://github.com/Thamil001/rdfs/issues"
  },
  "homepage": "https://github.com/Thamil001/rdfs",
  "contributes": {
    "commands": [
      {
        "command": "rdfs.toggleDevFolders",
        "title": "Rdfs"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "React Dev Folder Settings",
      "properties": {
        "rdfs.hiddenItems": {
          "type": "object",
          "default": {
            "**/node_modules": true,
            "**/dist": true,
            "**/build": true,
            "**/README.md": true,
            "**/package.json": true,
            "**/package-lock.json": true,
            "**/eslint.config.js": true,
            "**/vite.config.js": true,
            "**/.gitignore": true
          },
          "description": "Choose which files/folders should be hidden in React Dev Mode"
        }
      }
    },
    "keybindings": [
      {
        "command": "rdfs.toggleDevFolders",
        "key": "ctrl+alt+r",
        "mac": "cmd+alt+r",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/vscode": "^1.108.1",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  }
}